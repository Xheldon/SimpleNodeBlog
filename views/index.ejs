<%include ./public/head%>
<%include ./public/header%>
<div class="container">
    <div class="row">
        <div class="col-xs-9">
            <%for(var i=0,length = data.length;i<length;i++){%>
            <article>
                <div class="h6">
                    <button class="btn btn-primary btn-xs">原创</button>
                    <a href="/post/<%=data[i]['_id']%>"><%=data[i]['postTitle']%></a>
                </div>
                <span class="small"><kbd><%=data[i]['postDate']%></kbd></span>
                <div class="content">
                    <%=data[i]['postContent']%>
                </div>
            </article>
            <hr>
            <%}%>
        </div>
        <%include ./public/side%>
    </div>
</div>



<script src="/js/index.js"></script>
<%include ./public/foot%>
